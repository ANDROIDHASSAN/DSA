#101DaysOfDSA #MajorityElement #ProblemSolving #CodingJourney #Day26 #DSAChallenge #KeepLearning

class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int count = 1;
        int res = nums[0];
        for(int i=1;i<nums.size();i++)
        {
            if(count == 0)
            {
                res = nums[i];
            }
            if(res == nums[i]){
                count++;
            }
            else{
                count--;
            }
        }
        return res;
    }
};
